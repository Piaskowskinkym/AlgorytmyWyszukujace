            </Button.RenderTransform>
        </Button>
        <TextBlock HorizontalAlignment="Left" Margin="10,51,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Height="351" Width="185"/>
        <TextBlock HorizontalAlignment="Left" Margin="200,51,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Height="25" Width="239"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="200,20,0,0" VerticalAlignment="Top" Width="239"/>
        <ComboBox HorizontalAlignment="Left" Margin="516,42,0,0" VerticalAlignment="Top" Width="216"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="516,15,0,0" VerticalAlignment="Top" Width="239"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,186,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Height="170" Width="375"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="213,155,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
